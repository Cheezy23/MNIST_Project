    "# Now we can normalise the data\n",
    "# We do this so that all the values are between 0 and 1 instead of 0 and 255\n",
    "\n",
    "y_train = tf.keras.utils.normalize(y_train, axis=1)\n",
    "x_train = tf.keras.utils.normalize(x_train, axis=1)\n"
   ]
